package com.java.arizzz;

public class C6 {

	
	
	
	    public static void main(String[] args) {
	        int[] array = {12, 45, 67, 89, 34, 7};
	        int max = array[0];
	        for (int num : array) {
	            if (num > max) {
	                max = num;
	            }
	        }
	        System.out.println("The largest element is: " + max);
	    }
	}

	
	

